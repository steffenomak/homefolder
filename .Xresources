xterm*vt100*geometry:           81x30
xterm*faceName:           Monospace:size=11:style=Regular
!xterm*faceName:            inconsolata:size=14
xterm*font: fixed
xterm*boldFont: fixed
xterm*boldMode: false

xterm*loginShell: true

xterm*termName: xterm-256color
xterm*dynamicColors:      true
xterm*utf8:               2
xterm*eightBitInput:      false
xterm*saveLines:          2000
xterm*toolBar:            true

!Xft.dpi: 98
!Xft.hinting: 2
!Xft.antialias: 2
!Xft.hintstyle: hintfull
!Xft.rgba: rgba

*background: #222222
*foreground: #babdb6
! Black (not tango) + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #3070cd
*color12: #4a90ea
! DarkMangenta + Mangenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff
